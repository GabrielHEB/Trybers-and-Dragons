{
  "name": "trybers-and-dragons",
  "version": "1.0.0",
  "description": "Projeto Trybers and Dragons - Simulação de RPG com interatividade",
  "main": "src/index.ts",
  "scripts": {
    "start": "ts-node src/index.ts",
    "deploy": "gh-pages -d src",
    "test": "jest --runInBand --verbose",
  "dev": "nodemon --watch \"./src/**\" --ext \"ts,js\" --ignore \"src/**/*.spec.ts,src/**/*.json\" --exec \"ts-node src/index.ts\"",
  "pretest": "rm -rf ./dist",
  "pretest:fast": "rm -rf ./dist",
  "test:fast": "jest --runInBand --verbose --bail",
  "lint": "eslint . --ext .js,.jsx,.ts,.tsx"
  },

  "repository": {
    "type": "git",
    "url": "https://github.com/GabrielHEB/Trybers-and-Dragons.git"
  },
  "keywords": ["HTML", "CSS", "JavaScript", "TypeScript", "RPG", "Trybers-and-Dragons"],
  "author": "Gabriel Henrique",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/GabrielHEB/Trybers-and-Dragons/issues"
  },
  "homepage": "https://GabrielHEB.github.io/Trybers-and-Dragons/",
  "devDependencies": {
    "@types/node": "^16.11.7",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.3",
    "gh-pages": "^4.0.0",
    "nodemon": "^2.0.22",
    "ts-node": "^10.9.1",
    "typescript": "^4.4.3"
  },
  "dependencies": {}
}
